public class Finally_block {

	public static void main(String[] args) {
		int a = 1;
		int b = 0;
		try {
			System.out.println(a / b);
		} catch (Exception e) {
			System.out.println("exception handled");
		}
		// finallu bloce is work always
		// if code dont have exception finally block work
		// code have exception also fibally block work
		finally {
			System.out.println("exception is there");
		}
		System.out.println("rest of the code work");
	}
}
